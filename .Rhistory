log_summed_exps <- function(x){
## definition of log_summed_exps:
## log(sum_i=1^n(e^x_i)) =
## x_1 + log(1 + sum_j=2^(n-1)e^(x_j-x_n))
n <- length(x)
sort(x, decreasing = TRUE)
sum_log <- 0
for(i in 2:n-1){
sum_log <- sum_log + exp(x[i]-x[n])
}
answ <- x[1] + log(1+sum_log)
return(answ)
}
log_summed_exps(seq(1, 2000))
